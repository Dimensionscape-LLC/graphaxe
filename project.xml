<?xml version="1.0" encoding="utf-8"?>
<project>	
	<meta title="graphaxe" package="graphaxe" version="1.0.0" company="Christopher Speciale" />
	
	<app main="Main" file="graphaxe" path="bin" />
	
	<window background="#000000" fps="60" />
	<window width="800" height="480" unless="mobile" />
	<window orientation="landscape" vsync="false" antialiasing="0" if="cpp" />
	
	<source path="src" />
	<haxelib name="openfl" />
	
	<icon path="ico/icon_48.png" />
	
	<define name="no_openfl_entry_point" />

	<assets path="ico" />

	<!--
	<haxeflag name="--macro" value="include('bf.core')"/>
 	-->

</project>